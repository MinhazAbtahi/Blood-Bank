<UserControl x:Class="Blood_Bank.UI.Start"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Blood_Bank.UI"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="800">
    <Grid Name="StartRoot" Background="White">
        <Image Source="/Blood Bank;component/Resources/cover1.png" Margin="10,10,10,248"/>
        <Image Source="/Blood Bank;component/Resources/cover4.png" Margin="10,333,366,61"/>
        <Button x:Name="SignInButton" Background="WhiteSmoke" Content="Sign In" HorizontalAlignment="Left" Margin="485,388,0,0" VerticalAlignment="Top" Width="200" Click="SignInButton_Click" FontSize="24" Height="45"/>
        <Button x:Name="signUpButton" Background="WhiteSmoke" Content="Sign Up" HorizontalAlignment="Left" Margin="485,438,0,0" VerticalAlignment="Top" Width="200" Click="signUpButton_Click" FontSize="24" Height="45"/>
        <GroupBox x:Name="groupBox" Header="" HorizontalAlignment="Left" Margin="439,352,0,0" VerticalAlignment="Top" Height="159" Width="286"/>

    </Grid>
</UserControl>
